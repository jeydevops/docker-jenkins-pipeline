node {
   // Mark the code checkout 'stage'....
   stage 'Checkout'
   checkout scm

   stage 'Build application'

   stage 'Build'

   def mvnHome = tool 'M3'
   sh "${mvnHome}/bin/mvn clean install"

   // Mark the code build 'stage'....
